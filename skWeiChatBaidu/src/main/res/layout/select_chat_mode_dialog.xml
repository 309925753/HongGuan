<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="@drawable/bg_for_shatup"    android:orientation="vertical">    <TextView        android:id="@+id/no_gag"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:gravity="center"        android:padding="9dp"        android:text="@string/msg_transmission_public"        android:textColor="@color/app_black"        android:textSize="@dimen/one_level_title" />    <TextView        android:id="@+id/one_gag"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@drawable/list_selector_background_ripple"        android:gravity="center"        android:padding="9dp"        android:text="@string/msg_transmission_desed"        android:textColor="@color/app_black"        android:textSize="@dimen/one_level_title" />    <TextView        android:id="@+id/two_gag"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@drawable/list_selector_background_ripple"        android:gravity="center"        android:padding="9dp"        android:text="@string/msg_transmission_aes"        android:textColor="@color/app_black"        android:textSize="@dimen/one_level_title" />    <TextView        android:id="@+id/three_gag"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@drawable/list_selector_background_ripple"        android:gravity="center"        android:padding="9dp"        android:text="@string/msg_transmission_asymmetric_aes"        android:textColor="@color/app_black"        android:textSize="@dimen/one_level_title" /></LinearLayout>